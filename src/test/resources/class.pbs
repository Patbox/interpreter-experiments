print("Class test started!");

class Test {
    test = 5;
    hello;

    constructor(hello) {
        this.hello = hello;
    }

    print() {
        Global.print(this.hello);
    }


    static test() {
        print("static works!");
    }
}

Test.test();

var value = Test("World");

value.print();

